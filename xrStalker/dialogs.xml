    <dialog id="actor_break_dialog" priority="-1">

      <phrase_list>

          <phrase id="0">

                  <text>See you.</text>

                  <action>dialogs.break_dialog</action>

          </phrase>                                                                                               

      </phrase_list>

    </dialog>

    

      <dialog id="general_pda_talk" pda="1">

      <precondition>dialogs.is_bad_relation</precondition>

      <phrase_list>

            <phrase id="0">

                  <text>Hi. Got a minute?</text>

                  <next>1</next>

                  <next>11</next>

            </phrase>                                                                                               

            <phrase id="1">

                  <text>Yes. What can I do?</text>

                  <next>2</next>               

                  <next>3</next>

                  <next>10</next>

            </phrase>

            <phrase id="11">

                  <text>Busy. Back off.</text>

            </phrase>

            <phrase id="10">

                  <text>I disconnect. Bye.</text>

            </phrase>

            <phrase id="2">

                  <text>Got business, let`s meet up?</text>

                  <next>21</next>

                  <next>22</next>

            </phrase>

            <phrase id="21">

                  <precondition>dialogs.want_trade</precondition>

                  <text>Roger. Waiting for you here. The coordinates are in your PDA.</text>

                  <action>dialogs.trade_init</action>

                  <next>10</next>

            </phrase>

            <phrase id="22">

                  <precondition>dialogs.dont_want_trade</precondition>

                  <text>No. Not today.</text>

                  <next>10</next>

            </phrase>

            <phrase id="3">

                  <text>Help me!</text>

                  <next>31</next>

                  <next>32</next>

            </phrase>

            <phrase id="31">

                  <text>I`m coming.</text>

                  <action>dialogs.helper_init</action>

                  <next>10</next>

            </phrase>

            <phrase id="32">

                  <text>I can`t. Have enough on my plate.</text>

                  <next>10</next>

            </phrase>   

      </phrase_list>

      </dialog>

 

 

    <dialog id="stalker_helper">

        <phrase_list>

            <phrase id="0">

                <text>What`s wrong?</text>

                <next>1</next>

                <next>2</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="1">

                <precondition>dialogs.is_medikit_present</precondition>

                <text>Bother, share with me your medkit, I`m dying!</text>

                <next>11</next>

                <next>12</next>

                <next>13</next>

                <next>14</next>

            </phrase>

            <phrase id="11">

                <precondition>dialogs.is_good_relation</precondition>

                <text>Here, thank me later.</text>

                <action>dialogs.drop_medikit</action>

                <next>1</next>

                <next>2</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="12">

                <precondition>dialogs.is_health_good</precondition>

                <text>Hey, you look healthy and wealthy to me.</text>

                <next>1</next>

                <next>2</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="13">

                <precondition>dialogs.is_medikit_no_present</precondition>

                <text>I have non, sorry.</text>

                <next>1</next>

                <next>2</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="14">

                <precondition>dialogs.is_bad_relation</precondition>

                <text>No free meals. 300 coins - and it`s yours.</text>

                <next>141</next>

                <next>142</next>

            </phrase>

            <phrase id="141">

                <text>Take the dough, cheapjack. Give me medkit.</text>

                <next>1411</next>

            </phrase>

            <phrase id="1411">

                <text>Deal.</text>

                <action>dialogs.sell_medikit</action>

                <next>1</next>

                <next>2</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="142">

                <text>I don`t have this money.</text>

                <next>1412</next>

            </phrase>

            <phrase id="1412">

                <text>Well, I`m not Salvation Army.</text>

                <next>1</next>

                <next>2</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="2">

                <text>Need gun, give me a hand, brother?</text>

                <next>21</next>

                <next>22</next>

            </phrase>

            <phrase id="21">

                <precondition>dialogs.have_extra_weapon</precondition>

                <text>No problem, choose. I`ll give at cost.</text>

                <action>dialogs.trade_help</action>

                <next>1</next>

                <next>2</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="22">

                <precondition>dialogs.no_have_extra_weapon</precondition>

                <text>Sorry, nothing to give.</text>

                <next>1</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="3">

                <text>Need backup. Give me a hand?</text>

                <next>31</next>

                <next>32</next>

            </phrase>

            <phrase id="31">

                <precondition>dialogs.is_good_relation</precondition>

                <text>Well, let`s go and see the spooky guy.</text>

                <action>dialogs.activate_helper_follow</action>

                <next>1</next>

                <next>2</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="32">

                <precondition>dialogs.is_bad_relation</precondition>

                <text>No, not me.</text>

                <next>1</next>

                <next>2</next>

                <next>3</next>

                <next>10</next>

            </phrase>

            <phrase id="10">

                <text>That's all, thanks.</text>

                <action>dialogs.disable_helper_2</action>

                <next>101</next>

            </phrase>

            <phrase id="101">

                <text> All right then.</text>

                <action>dialogs.disable_helper</action>

            </phrase>

        </phrase_list>

    </dialog>  

           

    <dialog id="stalker_wounded_friend">

        <phrase_list>

            <phrase id="0">

                <text>Bother, share with me your medkit, I`m dying!</text>

                <next>1</next>

                <next>2</next>

                <next>3</next>

            </phrase>

            <phrase id="1">

                <precondition>dialogs.medikit_yes</precondition>

                <text>Here, we`ll get even later</text>

                <next>11</next>

            </phrase>

            <phrase id="11">

                <text>Thanks, brother.</text>

                <action>dialogs.transfer_medikit</action>

                <action>dialogs.disable_wounded</action>

            </phrase>

            <phrase id="2">

                <precondition>dialogs.medikit_yes</precondition>

                <text>I can sell it.</text>

                <next>21</next>

                <next>22</next>

            </phrase>

            <phrase id="21">

                <precondition>dialogs.have_money</precondition>

                <text>OK, I`ll need no money on the other side.</text>

                <action>dialogs.wounded_trade</action>

                <action>dialogs.sell_medikit</action>

            </phrase>

            <phrase id="22">

                <precondition>dialogs.not_have_money</precondition>

                <text>I have no money.</text>

            </phrase>

            <phrase id="3">

                <precondition>dialogs.medikit_no</precondition>

                <text>Sorry, I have no medkit.</text>

            </phrase>

        </phrase_list>

    </dialog>

 

</game_dialogs>
