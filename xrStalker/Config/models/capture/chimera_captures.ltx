;-------------------------------------------------------------------------------
;------ disable params ---------------------------------------------------------

[disable]
linear_factor		= 1.0
angular_factor		= 1.0

;-------------------------------------------------------------------------------
;------ particle bones ---------------------------------------------------------

[particle_bones]
bone 				= bip01_head,0,0,0
bone 				= bip01_spine,0,0,0
bone 				= bip01_pelvis,0,0,0
bone 				= bip01_spine1,0,0,0

;-------------------------------------------------------------------------------
;------ foot bones -------------------------------------------------------------

[foot_bones]
front_left 			= bip01_l_finger0
front_right 		= bip01_r_finger0
back_right 			= bip01_l_finger0
back_left 			= bip01_r_finger0

;-------------------------------------------------------------------------------
;------ capture params ---------------------------------------------------------

[capture]
bone				= bip01_head			; bone for capture
distance			= 0.05					; distance - less this distance between bone and capture taget object is captured
time_limit       	= 100		        	; max time for trying to capture object
pull_force       	= 3000.					; max force used to bring the taget to capture bone
pull_distance   	= 0.5					; max distance taget can be puled
velocity_scale   	= 0.1					; velocity scale for pulling 1- normal
capture_force    	= 100000.				; max force for holding taget

;-------------------------------------------------------------------------------
;------ End params -------------------------------------------------------------