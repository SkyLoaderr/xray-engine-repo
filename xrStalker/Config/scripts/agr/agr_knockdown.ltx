[logic]
active = sr_idle

[sr_idle]
on_info = {+agr_knockdown} sr_idle@1

[sr_idle@1]
on_timer = 300 | sr_idle@2 %=agr_knockdown =run_cam_effector(dis_consciousness) =disable_ui%

[sr_idle@2]
on_timer = 3000 | sr_idle@3 %=run_postprocess(agr_u_fade)%

[sr_idle@3]
on_timer  = 5000 | nil  %=enable_ui =teleport_actor(kat_teleport_to_dark_city_position:kat_teleport_to_dark_city_orientation)%