    <a_static x="250" y="-3" width="32" height="32" frames="16" duration="1000" columns="8" frame_width="32" frame_height="32" cyclic="1" autoplay="1">
        <texture>ui\ui_pda_anims_04</texture>
    </a_static>

    <!-- ENCYCLOPEDIA INFO LIST -->
    <info_list x="20" y="87" width="423" height="374" item_height="15" right_ident="5">
