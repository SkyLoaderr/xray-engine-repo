    <left_frame_window x="313" y="0" width="448" height="475" a="80">
		<texture>ui_pda_frame_sub</texture>        
    </left_frame_window>

    <!-- INFO BACKGROUND HEADER -->
    <left_frame_line x="8" y="17" width="434" height="21">
        <title x="10" y="1" width="435" height="18">
            <text font="letterica16" r="255" g="255" b="255"/>
        </title>
		<texture>ui_string_03</texture>        
    </left_frame_line>

    <!-- CURRENT ARTICLE HEADER -->
	<article_header_static x="22" y="48" width="430" height="22">
		<text font="letterica25" r="225" g="225" b="250"></text>
	</article_header_static>

    <!-- LIST BACKGROUND -->
    <right_frame_window x="0" y="0" width="298" height="475" a="80">
		<texture>ui_pda_frame_sub</texture>        
    </right_frame_window>

    <!-- LIST HEADER -->
    <right_frame_line x="8" y="16" width="284" height="27">
