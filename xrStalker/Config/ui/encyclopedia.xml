    <!-- ENCYCLOPEDIA ITEM IMAGE INFO -->
    <!-- MASK -->
    <mask_frame_window x="0" y="0" width="500" height="500" a="70">
		<texture>ui_pda_frame2</texture>		
	</mask_frame_window>
  	
  	<!-- AUTOSTATICS FOR LEFT FRAME WINDOW -->
	
	<!-- LEFT LADDER -->
	<left_auto_static x="8" y="41" width="4" height="430">
		<texture>ui\ui_pda_vertical_line_r_l</texture>
	</left_auto_static>
	<!-- RIGHT LADDER -->
