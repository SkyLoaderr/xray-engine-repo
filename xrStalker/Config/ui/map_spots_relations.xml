	<enemy_location hint="enemy" no_offline="1">
		<mini_map spot="blue_mini_spot"		pointer="quest_pointer"/>
	</enemy_location>

	<neutral_location hint="neutral">
		<mini_map  spot="neutral_mini_spot"/>
	</neutral_location>

	<friend_location hint="friend">
		<mini_map  spot="friend_mini_spot"/>
	</friend_location>

	<deadbody_location hint="deadbody">
		<mini_map spot="green_mini_spot"	pointer="quest_pointer"/>
	</deadbody_location>

	<enemy_mini_spot width="5" height="5" stretch="1" alignment="c" xform_anim="map_spot_rel_xform" xform_anim_cyclic="0">
		<texture r="255" g="0" b="0">ui_minimap_point</texture>
	</enemy_mini_spot>
	
	<neutral_mini_spot width="5" height="5" stretch="1" alignment="c" xform_anim="map_spot_rel_xform" xform_anim_cyclic="0"> 
		<texture r="255" g="200" b="0">ui_minimap_point</texture>
	</neutral_mini_spot>

	<friend_mini_spot width="5" height="5" stretch="1" alignment="c" xform_anim="map_spot_rel_xform" xform_anim_cyclic="0"> 
		<texture r="50" g="255" b="0">ui_minimap_point</texture>
	</friend_mini_spot>
	
	<deadbody_mini_spot width="5" height="5" stretch="1" alignment="c" xform_anim="map_spot_rel_xform" xform_anim_cyclic="0"> 
		<texture r="168" g="168" b="168">ui_minimap_point</texture>
	</deadbody_mini_spot>


