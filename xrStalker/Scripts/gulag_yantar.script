function load_states(gname, type)
    if type == "yantar_zombies" then
		return function(gulag)
			return 0
		end
    end
    if type == "yan_scientist" then
		return function(gulag)
			if db.actor:has_info("yan_scientist_gulag_state_2") then 
				return 1
			end
			if db.actor:has_info("yan_scientist_gulag_state_3") then 
				return 2
			end
			return 0
		end
    end
	
	if type == "yan_tonnel_snorks" then
		return function(gulag)
			return 0
		end
	end

end



