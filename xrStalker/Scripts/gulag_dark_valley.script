function load_states(gname, type)
    --- Soldiers -------------------------------------------
	if type == "val_lager_bandits"      then    return check_state_lager_bandits
	elseif type == "val_monolith_base"  then    return check_state_monolith_base
	elseif type == "val_war_soldiers"   then    return check_state_war_soldiers
	--- Monsters -------------------------------------------
	--- Dogs
	elseif type == "val_dogs_nest1"     then    return check_state_dogs_nest1
	elseif type == "val_dogs_nest2"     then    return check_state_dogs_nest2
	elseif type == "val_dogs_nest3"     then    return check_state_dogs_nest3
	elseif type == "val_dogs_nest4"     then    return check_state_dogs_nest4
	elseif type == "val_dogs_nest5"     then    return check_state_dogs_nest5
	--- Fleshes
	elseif type == "val_fleshes_nest1"  then    return check_state_fleshes_nest1
	elseif type == "val_fleshes_nest2"  then    return check_state_fleshes_nest2
	elseif type == "val_fleshes_nest3"  then    return check_state_fleshes_nest3
	elseif type == "val_fleshes_nest4"  then    return check_state_fleshes_nest4
	elseif type == "val_fleshes_nest5"  then    return check_state_fleshes_nest5
	--- Boars
	elseif type == "val_boars_nest1"    then    return check_state_boars_nest1
	elseif type == "val_boars_nest2"    then    return check_state_boars_nest2
	elseif type == "val_boars_nest3"    then    return check_state_boars_nest3
	elseif type == "val_boars_nest4"    then    return check_state_boars_nest4
	elseif type == "val_boars_nest5"    then    return check_state_boars_nest5
	end

	return nil
end

function check_state_lager_bandits(gulag)
	for k,v in gulag.Object do
		local enemy = v:best_enemy()
		if enemy ~= nil then
			enemy_type = get_clsid(enemy)
			if enemy_type == clsid.actor and
				utils.npc_in_zone(db.actor, db.zone_by_name["val_lager_bandits_restrictor"]) then
				return state_alarm_actor_inside
			end
			if enemy_type == clsid.actor or
  	   			enemy_type == clsid.soldier or
	   			enemy_type == clsid.script_stalker or
   				enemy_type == clsid.trader
   			then
   				gulag.alarm_start = game.get_game_time()
   				return state_alarm
   			end
		end
	end		
	
	if gulag.alarm_start == nil or game.get_game_time():diffSec(gulag.alarm_start) > 900 then
		return state_normal -- Ó·˚˜ÌÓÂ ÒÓÒÚÓˇÌËÂ
	end
	return state_alarm -- ÚÂ‚Ó„‡
end

function check_state_monolith_base(gulag)
	return state_normal
end

function check_state_war_soldiers(gulag)
	if db.actor:has_info("val_opendoor_chase_start") then
		return state_normal
	end
	return state_fight
end

