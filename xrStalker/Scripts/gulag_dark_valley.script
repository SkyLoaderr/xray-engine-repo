function checkStalker(npc_community, gulag_type)
	if      gulag_type == "val_lager_bandits"   then    return npc_community == "bandit"
	elseif  gulag_type == "val_monolith_base"   then    return npc_community == "monolith"
	elseif  gulag_type == "val_war_soldiers"    then    return npc_community == "military"
	end    

	return false
end

function checkMonster(npc_community, gulag_type)
    if      gulag_type == "val_dogs_nest1" or
            gulag_type == "val_dogs_nest2" or
            gulag_type == "val_dogs_nest3" or
            gulag_type == "val_dogs_nest4" or
            gulag_type == "val_dogs_nest5"      then return npc_community == "dog"

    elseif  gulag_type == "val_fleshes_nest1" or
            gulag_type == "val_fleshes_nest2" or
            gulag_type == "val_fleshes_nest3" or
            gulag_type == "val_fleshes_nest4" or
            gulag_type == "val_fleshes_nest5"   then return npc_community == "flesh"

    elseif  gulag_type == "val_boars_nest1" or
            gulag_type == "val_boars_nest2" or
            gulag_type == "val_boars_nest3" or
            gulag_type == "val_boars_nest4" or
            gulag_type == "val_boars_nest5"     then return npc_community == "boar"
    end

    return false
end
