function load_job(sj, gname, type, squad, groups)
    ------Stalkers-------------------------------------------------------
	if      type == "val_lager_bandits" then    load_job_lager_bandits  (sj, gname, type, squad, groups)
	elseif  type == "val_monolith_base" then    load_job_monolith_base  (sj, gname, type, squad, groups)
	elseif  type == "val_war_soldiers"  then    load_job_war_soldiers   (sj, gname, type, squad, groups)
    ------Monsters-------------------------------------------------------
    --- Dogs
	elseif  type == "val_dogs_nest1"    then    load_job_dogs_nest1     (sj, gname, type, squad, groups)
	elseif  type == "val_dogs_nest2"    then    load_job_dogs_nest2     (sj, gname, type, squad, groups)
	elseif  type == "val_dogs_nest3"    then    load_job_dogs_nest3     (sj, gname, type, squad, groups)
	elseif  type == "val_dogs_nest4"    then    load_job_dogs_nest4     (sj, gname, type, squad, groups)
	elseif  type == "val_dogs_nest5"    then    load_job_dogs_nest5     (sj, gname, type, squad, groups)
	--- Fleshes
	elseif  type == "val_fleshes_nest1" then    load_job_fleshes_nest1  (sj, gname, type, squad, groups)
	elseif  type == "val_fleshes_nest2" then    load_job_fleshes_nest2  (sj, gname, type, squad, groups)
	elseif  type == "val_fleshes_nest3" then    load_job_fleshes_nest3  (sj, gname, type, squad, groups)
	elseif  type == "val_fleshes_nest4" then    load_job_fleshes_nest4  (sj, gname, type, squad, groups)
	elseif  type == "val_fleshes_nest5" then    load_job_fleshes_nest5  (sj, gname, type, squad, groups)
	--- Boars
	elseif  type == "val_boars_nest1"   then    load_job_boars_nest1    (sj, gname, type, squad, groups)
	elseif  type == "val_boars_nest2"   then    load_job_boars_nest2    (sj, gname, type, squad, groups)
	elseif  type == "val_boars_nest3"   then    load_job_boars_nest3    (sj, gname, type, squad, groups)
	elseif  type == "val_boars_nest4"   then    load_job_boars_nest4    (sj, gname, type, squad, groups)
	elseif  type == "val_boars_nest5"   then    load_job_boars_nest5    (sj, gname, type, squad, groups)
	end
end

