function dump_time(t)
printf("----dump time start")
	printf(t:dateToString(time_struct.DateToDay))
	printf(t:dateToString(time_struct.DateToMonth))
	printf(t:dateToString(time_struct.DateToYear))
	
	printf(t:timeToString(time_struct.TimeToHours))
	printf(t:timeToString(time_struct.TimeToMinutes))
	printf(t:timeToString(time_struct.TimeToSeconds))
	printf(t:timeToString(time_struct.TimeToMilisecs))
printf("----dump time end")
printf("")
end

