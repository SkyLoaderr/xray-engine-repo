function proceed()
    local actor = db.actor
	-- Bar Rostok
	-- выдача информации с флешки профессора
	if actor:dont_has_info("bar_ecolog_research_normal") and
	   actor:dont_has_info("bar_ecolog_research_corrupted") and
	   actor:object("bar_ecolog_flash") 
	then
		if actor:has_info("bar_rescue_research_corruped") then
			actor:give_info_portion("bar_ecolog_research_corrupted")
		else
			actor:give_info_portion("bar_ecolog_research_normal")
		end
	end
end

function process_info_portion(info_id)
	if info_id == "bar_freedom_attack_ecolog" then
		if news_manager.send_tip(db.actor, "bar_freedom_attack", nil, "freedom", nil, 509) then
			db.actor:give_info_portion("bar_freedom_defence_ecolog")
		end
		return true
	elseif info_id == "bar_dolg_petrenko" then
		level_tasks.add_location(506, "green_location", "bar_petrenko")
		return true		
	elseif info_id == "bar_dolg_community_start" then
		level_tasks.remove_location(506, "green_location", "bar_petrenko")
		return true		
	-- убирание отметки Волкодава
	elseif info_id == "bar_heli_scene_volkodav_die" then
		level_tasks.remove_location_safe(509, "green_location")
		return true		
	elseif info_id == "bar_rescue_research_start" then
		level_tasks.remove_location_safe(509, "green_location")
		return true		
	elseif info_id == "bar_kill_professor_start" then
		level_tasks.remove_location_safe(509, "green_location")
		return true		
	-- убирание отметки Профессора
	elseif info_id == "bar_heli_scene_professor_die" then
		level_tasks.remove_location_safe(503, "green_location")
		return true		
	elseif info_id == "bar_rescue_research_start" then
		level_tasks.remove_location_safe(503, "green_location")
		return true		
	elseif info_id == "bar_kill_professor_start" then
		level_tasks.remove_location_safe(503, "green_location")
		return true		

	end
	return false
end

