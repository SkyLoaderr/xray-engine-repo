
function aes_segeant_kill (stalker, player)
    local h = hit ()
    h.power = 100
    h.direction = vector():set (0, -1, 0)
    h.impulse = 1
    h.draftsman = stalker
    h.type = hit.strike
    player:hit (h)
end

function aes_set_mapspots (stalker, player)
    level_tasks.add_location (1102, "crlc_mdl", "aes_sarcofag_hint")
end