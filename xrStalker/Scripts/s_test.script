function main ()

	local boar = get_level_object ("boar")
	local way = patrol ("way")

	boar:script (true, s_test.script_name ())

	action (boar, move (move.act_stand_idle, move.default, vector ():set (0, 0, 0)), cond (cond.time_end, time_infinite))
	
	wait (5000)
	reset_action (boar, s_test.script_name ())
	action (boar, move (move.act_run, move.force_type, way), cond (cond.move_end))
	
	while boar:action () do wait () end

	boar:script (false, s_test.script_name ())

 end