function main ()

	local eater = get_level_object ("eater")
	local kolbasa = get_level_object ("kolbasa")

	eater:script (true, s_test.script_name ())
	
	action (eater, object (kolbasa, object.activate), cond (cond.time_end, 5000))
	action (eater, object (kolbasa, object.fire1), cond (cond.time_end, 5000))
	action (eater, object (kolbasa, object.reload), cond (cond.time_end, 5000))
	action (eater, object (kolbasa, object.deactivate), cond (cond.time_end, 5000))

	while true do wait () end	
end