function main ()

	local n1 = get_level_object ("naparnik1")
	local n2 = get_level_object ("naparnik2")

	n1:script (true, s_test.script_name ())
	n1:script (true, s_test.script_name ())

	action (n1, move (move.standing, move.walk, move.line, patrol ("boar_way")), 
			look (look.search), anim (anim.free), cond (cond.move_end))
	action (n2, move (move.standing, move.walk, move.line, patrol ("boar_way")), 
			look (look.search), anim (anim.free), cond (cond.move_end))


	local stalker = get_level_object ("stalker")
	local torch = get_level_object ("torch")
	stalker:script (true, s_test.script_name ())
	action (stalker, object (torch, object.activate), cond (cond.time_end, time_infinite))

	while stalker:action () do wait () end

	stalker:script (false, s_test.script_name ())
end