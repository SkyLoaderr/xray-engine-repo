function main ()

    local niva = get_level_object ("niva")

    niva:script (true, s_test.script_name ())


    action (niva, move (move.on + move.up + move.fwd, 0.1), object ("right_light", object.activate),
            object ("left_light", object.activate), cond (cond.time_end, 8200))

    while niva:action () do wait () end

    niva:script (false, s_test.script_name ())
end