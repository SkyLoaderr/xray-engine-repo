function main()
	local actor   	= get_actor()
	for i=1,10 do
		local 			ps = particles("nature\\fog_stormy",true)
		local			v = actor:position()
		v.x			= v.x + 2*(math.random(0,100)/100 - 0.5)
		v.y			= v.y + 1.5
		v.z			= v.z + 2*(math.random(0,100)/100 - 0.5)
		ps:play_at_pos		(v);
		wait			(5000)
	end
end
