			<text font="letterica16" r="255" g="255" b="255"></text>
		</title>
		<base_texture>ui\ui_string_03</base_texture>
	</msglog_frame_line>

	<!-- CHARACTER ICON BACKGROUND -->
	<chicon_frame_window x="539" y="62" width="298" height="475" a="80">
		<base_texture>ui\ui_pda_frame_sub</base_texture>
	</chicon_frame_window>

	<!-- HEADER CHAR ICON -->
	<chicon_frame_line x="8" y="16" width="284" height="27">
        <title x="15" y="1" width="545" height="440">
            <text font="letterica25" r="225" g="225" b="250">dummy</text>
        </title>
		<base_texture>ui\ui_string_02</base_texture>
	</chicon_frame_line>
	
	<!--  ANIMATED STATIC -->
	<a_static x="250" y="-3" width="32" height="32"
			frames			= "16"
			duration		= "1000"
			columns			= "8"
			frame_width		= "32"
			frame_height	= "32"
			cyclic			= "1"
			autoplay		= "1"
	>
		<texture>ui\ui_pda_anims_05</texture>
	</a_static>

            
	<!-- OUR PHRASES BACKGROUND -->
	<ph_frame_window x="75" y="435" width="448" height="100" a="80">
		<base_texture>ui\ui_pda_frame_sub</base_texture>
	</ph_frame_window>

	<!-- OUR PHRASES HEADER -->
	<ph_frame_line x="8" y="17" width="434" height="21">
		<title x="10" y="1" width="435" height="18">
			<text font="letterica16" r="255" g="255" b="255"></text>
		</title>
		<base_texture>ui\ui_string_03</base_texture>
	</ph_frame_line>

	<!-- MASK FOR CHARACTER ICON -->	
	<mask_frame_window x="0" y="0" width="1" height="1">
