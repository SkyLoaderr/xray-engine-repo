	<character_info x="26" y="58" width="236" height="120"></character_info>

	<font font="letterica16" r="255" g="255" b="255"></font>
	<font font="letterica16" r="255" g="255" b="255"></font>
	
	<!-- MESSAGE LOG LIST -->
	<list x="20" y="40" width="470" height="320" item_height="16">
		<font font="letterica16" r="255" g="255" b="255"></font>
	</list>
	<!-- PHRASES LIST -->
	<list x="20" y="37" width="470" height="75" item_height="15">
		<font font="letterica16" r="255" g="255" b="255"></font>
	</list>

	<!-- MESSAGE LOG BACKGROUND -->
	<msglog_frame_window x="76" y="62" width="448" height="365" a="80">
		<base_texture>ui\ui_pda_frame_sub</base_texture>
	</msglog_frame_window>

	<!-- HEADER FOR MSGLOG -->
	<msglog_frame_line x="8" y="17" width="434" height="21">
