struct 	v2p
{
  half2 tc0: 		TEXCOORD0;	// Texture coordinates 	(for sampling maps)
};

//
uniform sampler2D  	s_accumulator;

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
// Purpose: set (1,1,1,1) if any out of four samples are > 0
//          input is always 0 or 1, but it is filtered
half4 	main		( v2p I )	: COLOR
{
	half mix	= tex2D	(s_accumulator, I.tc0);
	return mix>.1?1:0;
}
