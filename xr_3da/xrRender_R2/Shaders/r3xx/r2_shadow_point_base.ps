struct 	v2p_in
{
	half3 	lpos:	TEXCOORD0;	// Vector from light to point
};

//////////////////////////////////////////////////////////////////////////////////////////
uniform sampler2D 	s_base;

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
half4 	main	( v2p_in I )	: COLOR
{
	half 	range 	= length(I.lpos);
	return	half4	(range,range,range,range);
}
