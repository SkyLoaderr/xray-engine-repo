function get_task_info(task_id, fmt)
	local _info = GetTextureInfo(task_id..fmt, "default_task_icon"..fmt)
	local r = _info:get_rect()
	r.x2 = r.x2-r.x1
	r.y2 = r.y2-r.y1
	return _info:get_file_name(), r
end

