--Кабан бродит по мосту, отдыхает, кушает труп
module( "m_monster_ai" )

local boar = nil

function init()
    boar = m_monster_ai.garbage_boar( "jons_boar_1", this.script_name(), "boar_way_1", vector():set( 1, 0, 0 ) )
    boar:initialize()
end

function execute()
    return boar:execute()
end

function finalize()
    boar:finalize()
end