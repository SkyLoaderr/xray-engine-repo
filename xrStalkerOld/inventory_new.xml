<?xml version='1.0' encoding="UTF-8"?>
<window x="0" y="0" w="640" h="480" r="227" g="121" b="222" texture="1">

    <!-- BAG FRAME WINDOW -->
    <bag_static x="10" y="260" width="350" height="480">
		<texture>ui\ui_dg_inventory</texture>
        <text x="30" y="8" font="graffiti22" r="231" g="153" b="22">ui_inv_bag</text>
    </bag_static>
    
    <!-- MONEY INDICATOR ... it will be attached to BAG window. 
			So its cordinates is relative to left up corner of BAG window-->
    <money_static x="220" y="0" width="117" height="40">
		<texture>ui\ui_button_05</texture>
		<text x="20" y="8" font="graffiti22" r="238" g="153" b="26"></text>
    </money_static>

    <!-- ARTIFACT MERGER WINDOW -->
    <frame_window x="440" y="260" width="267" height="500">
		<texture>ui_frame</texture>        
    </frame_window>

    <!-- SLEEP PARAMS WINDOW -->
    <sleep_window x="370" y="672" width="260" height="53">
        <texture>ui\ui_dg_sleep</texture>
        <text>ui_inv_rest</text>
    </sleep_window>

    <!-- DESCRIPTION WINDOW -->
    <descr_static x="370" y="260" width="335" height="475">
        <texture> ui\ui_dg_info</texture>
        <text x="20" y="10" font="graffiti22" r="231" g="153" b="22">ui_inv_description</text>
    </descr_static>

    <!-- CHARACTER WINDOW -->
    <frame_window x="730" y="260" width="268" height="460">
		<texture>ui_frame</texture>        
        <left_top_texture x="5" y="10">ui\ui_inv_personal_over_t</left_top_texture>
        <title x="100" y="0" width="240" height="60">
			<text font="graffiti22" r="231" g="153" b="22">ui_inv_personal</text>
        </title>
    </frame_window>
    
    <!-- OUTFIT INFO -->
    <outfit_info_window x="20" y="36" width="268" height="460">
		<text font="letterica16" r="240" g="217" b="182"/>
	</outfit_info_window>

    <!-- BELT STATIC BACKGROUND -->
    <static x="10" y="140" width="1014" height="128">
        <texture> ui\ui_inv_belt</texture>
        <text x="25" y="2" font="graffiti22" r="231" g="153" b="22">ui_inv_belt</text>
    </static>

    <button x="600" y="715" width="150" height="40">
        <texture>ui\ui_button_01</texture>
        <text>Medkit</text>
    </button>
    <button x="800" y="715" width="150" height="40">
        <texture>ui\ui_button_01</texture>
        <text>Antirad</text>
    </button>
    <button x="800" y="615" width="150" height="40">
        <texture>ui\ui_button_01</texture>
        <text>Food</text>
    </button>

	<!-- Drop button -->
    <drop_button x="230" y="413" width="110" height="60" hR="255" hB="83" hG="179" highlight_text="1">
        <texture>ui\ui_cm_drop_over</texture>
        <text x="0" y="0" font="graffiti22" r="238" g="153" b="26">ui_inv_drop</text>
    </drop_button>
    
    <!-- BELT LIST -->
    <dragdrop_list x="27" y="174" width="1024" height="60"
            cell_width = "52" cell_height="52"
            rows_num="1" cols_num="18"/>
    <!-- BAG LIST -->
    <dragdrop_list x="30" y="35" width="400" height="350"
            cell_width = "40" cell_height="40"
            rows_num="18" cols_num="7" rows_num_view="10" unlimited="1"/>

    <!-- SLOTS LISTS -->
    <dragdrop_list x="23" y="14" width="120" height="120"
            cell_width = "52" cell_height="52"
            rows_num="2" cols_num="2"/>

    <dragdrop_list x="168" y="14" width="120" height="120"
            cell_width = "52" cell_height="52"
            rows_num="2" cols_num="2"/>

    <!-- ACTIVE WEAPON SLOT -->
    <dragdrop_list x="313" y="14" width="320" height="120"
            cell_width = "52" cell_height="52"
            rows_num="2" cols_num="6"/>

    <dragdrop_list x="670" y="14" width="100" height="120"
            cell_width = "52" cell_height="52"
            rows_num="2" cols_num="2"/>

    <dragdrop_list x="818" y="14" width="150" height="120"
            cell_width = "52" cell_height="52"
            rows_num="2" cols_num="3"/>

    <!-- OUTFIT SLOT -->
    <!-- <dragdrop_list x="828" y="314" width="150" height="220" -->
    <dragdrop_list x="858" y="314" width="86" height="260" 
            cell_width = "57" cell_height="57"
            rows_num="4" cols_num="4" show_grid="0"/>


    <!-- HEALTH BAR -->
    <progress_bar x="15" y="65" length="222" broad="20" horz="0" min="0" max="1000" pos="0">
        <texture width="17" height="222" length="222" r="255" g="0" b="0">ui\ui_inv_progress_blood</texture>
        <background width="17" height="222" >ui\ui_inv_progress_blood_empty</background>
    </progress_bar>

    <!-- SATIETY BAR -->
    <progress_bar x="36" y="65" length="222" broad="20" horz="0" min="0" max="1000" pos="0">
        <texture width="17" height="222" length="222" r="255" g="255" b="0">ui\ui_inv_progress_food</texture>
        <background x="0" y="0" width="17" height="222" >ui\ui_inv_progress_food_empty</background>
    </progress_bar>

    <!-- POWER BAR -->
    <progress_bar x="58" y="65" length="222" broad="20" horz="0" min="0" max="1000" pos="0">
        <texture width="17" height="222" length="222" r="0" g="255" b="0">ui\ui_inv_progress_end</texture>
        <background x="0" y="0" width="17" height="222" >ui\ui_inv_progress_end_empty</background>
    </progress_bar>

    <!-- RADIATION BAR -->
    <progress_bar x="79" y="65" length="222" broad="20" horz="0" min="0" max="1000" pos="0">
        <texture width="17" height="222" length="222" r="0" g="255" b="255">ui\ui_inv_progress_rad</texture>
        <background x="0" y="0" width="17" height="222" >ui\ui_inv_progress_rad_empty</background>
    </progress_bar>

    <time_static x="15" y="730" width="200" height="30">        
        <text x="20" y="8" font="medium" r="240" g="217" b="182" a="255">ui_inv_time</text>
	</time_static>

	<!-- Exit button -->
    <exit_button x="885" y="725" width="110" height="40" hR="255" hB="83" hG="179" highlight_text="1">
        <texture>ui\ui_button_06</texture>
        <text align="c" font="graffiti22" r="238" g="153" b="26">ui_inv_exit</text>
    </exit_button>

<action_sounds>
	<snd_open>interface\inv_open</snd_open>
	<snd_close>interface\inv_close</snd_close>
	<snd_item_to_slot>interface\inv_slot</snd_item_to_slot>
	<snd_item_to_belt>interface\inv_belt</snd_item_to_belt>
	<snd_item_to_ruck>interface\inv_ruck</snd_item_to_ruck>
	<snd_properties>interface\inv_properties</snd_properties>
	<snd_drop_item>interface\inv_drop</snd_drop_item>
	<snd_attach_addon>interface\inv_drop</snd_attach_addon>
	<snd_detach_addon>interface\inv_drop</snd_detach_addon>
</action_sounds>
</window>